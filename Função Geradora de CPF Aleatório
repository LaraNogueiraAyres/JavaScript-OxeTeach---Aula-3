function sorteio(min, max)
{ 
     min = Math.ceil(min);
     max = Math.floor(max);
     return Math.floor(Math.random() * (max - min) + min)
}

console.log(`${sorteio(0, 9)}${sorteio(0, 9)}${sorteio(0, 9)}.${sorteio(0, 9)}${sorteio(0, 9)}${sorteio(0, 9)}.${sorteio(0, 9)}${sorteio(0, 9)}${sorteio(0, 9)}-${sorteio(0, 9)}${sorteio(0, 9)}`);

